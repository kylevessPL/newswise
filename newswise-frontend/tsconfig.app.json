{
    "compilerOptions": {
        "outDir": "./out-tsc/app",
        "paths": {
            "rxjs": [
                "node_modules/rxjs"
            ],
            "rxjs/*": [
                "node_modules/rxjs/*"
            ],
            "stream": [
                "node_modules/stream-browserify"
            ],
            "http": [
                "node_modules/stream-http"
            ],
            "https": [
                "node_modules/https-browserify"
            ],
            "url": [
                "node_modules/url"
            ]
        }
    },
    "extends": "./tsconfig.json",
    "files": [
        "src/main.ts",
        "src/polyfills.ts"
    ],
    "include": [
        "src/**/*.d.ts"
    ]
}
